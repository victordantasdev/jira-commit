{
    "env": {
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint"
    ],
    "ignorePatterns": ["*.d.ts", "lib"],
    "rules": {
        "no-console": 0,
        "import/no-import-module-exports": 0,
        "import/extensions": 0,
        "import/no-unresolved": 0
    }
}
